        skrew("SKYPE.MacDownloadDisable", ["SKYPE.Accessibility", "SKYPE.browserInfo"], function(skypeAccessibility, skypeBrowserInfo)
        {
            if (skypeBrowserInfo.isMac105() || skypeBrowserInfo.isMacOld())
            {
                var macDownloadButton = $("p.action:has(a[data-dwn='download;Skype: Mac'])");
                var macNotificationMessage = $(".notification.noMobile.noDesktop");

                if (!!macDownloadButton.length)
                {
                    macDownloadButton.remove();
                }

                if (!!macNotificationMessage.length)
                {
                    macNotificationMessage.removeClass("noMobile noDesktop");
                }
            }
        });
    </script>





			
		
	
	
	
	
	
		
		
